OOF.Microstructure.Create_From_ImageFile(filename='examples/small.ppm', microstructure_name='small.ppm', height=automatic, width=automatic)
OOF.Windows.Graphics.New()
OOF.Skeleton.New(name='skeleton', microstructure='small.ppm', x_elements=4, y_elements=4, skeleton_geometry=QuadSkeleton(left_right_periodicity=False,top_bottom_periodicity=False))
OOF.Graphics_1.Layer.Select(n=4)
OOF.LayerEditor.LayerSet.Edit(window='Graphics_1', layer_number=4)
OOF.LayerEditor.LayerSet.DisplayedObject(category='Image', object='small.ppm:small.ppm')
OOF.LayerEditor.LayerSet.DisplayedObject(category='Skeleton', object='small.ppm:skeleton')
OOF.LayerEditor.LayerSet.Add_Method(method=SkeletonMaterialDisplay())
OOF.LayerEditor.LayerSet.Send(window='Graphics_1')
OOF.LayerEditor.LayerSet.Delete_Method(layer_number=0)
OOF.LayerEditor.LayerSet.Send(window='Graphics_1')
OOF.Graphics_1.Layer.Select(n=1)
OOF.LayerEditor.LayerSet.Edit(window='Graphics_1', layer_number=1)
OOF.LayerEditor.Settings.AutoSend(0)
OOF.LayerEditor.LayerSet.Replace_Method(layer_number=0, method=SkeletonMaterialDisplay())
OOF.LayerEditor.LayerSet.Replace_Method(layer_number=0, method=SkeletonEdgeDisplay(color=Gray(value=0.0),width=0))
OOF.LayerEditor.LayerSet.Send(window='Graphics_1')
OOF.LayerEditor.Settings.AutoSend(1)
OOF.LayerEditor.LayerSet.Replace_Method(layer_number=0, method=SkeletonMaterialDisplay())
OOF.LayerEditor.LayerSet.Send(window='Graphics_1')
